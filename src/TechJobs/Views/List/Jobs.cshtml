<h1>@ViewBag.title</h1>

<h3>@(ViewBag.jobsKVP.Count/5) Result(s)</h3>

@{
// TODO #3 - Use the same list display from Views/Search/Index.cshtml


<table>

    @foreach (KeyValuePair<string, string> jobKVP in ViewBag.jobsKVP)
    {
        <tr>
            <td><b>@jobKVP.Key</b></td>
            <td>@jobKVP.Value</td>
        </tr>

        if (jobKVP.Key.Equals("core competency"))
        {
            <tr>
                <td> </td>
            </tr>
            
        }

    }
</table>




}